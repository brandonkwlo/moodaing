{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Moodaing",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "storage", "contextMenus"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "16": "hippo.png",
    "32": "hippo.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
    }
  ]
}
